{
  "AWSEBDockerrunVersion": "2",
  "Logging": "/app/log",
  "Volumes": [
    {
      "HostDirectory": "/var/tmp",
      "ContainerDirectory": "/tmp"
    }
  ],
  "containerDefinitions": [
    {
      "name": "dropwizard",
      "image": "twdevops/java/8",
      "essential": true,
      "memory": 128,
      "mountPoints": [
        {
          "sourceVolume": "awseb-logs-dropwizard",
          "containerPath": "/app/log"
        }
      ]
    }
  ],
  "portMappings": [
    {
      "hostPort": 8080,
      "containerPort": 8080
    },
    {
      "hostPort": 8081,
      "containerPort": 8081
    }
  ]
}